# Copy this file to `.env` and adjust values for local development.
# Units configuration
DEFAULT_UNITS=mm

# LLM provider configuration
# AI_AUTOCAD_PROVIDER can be "mock", "openai", etc. Defaults to "mock" for tests.
AI_AUTOCAD_PROVIDER=mock
# API key is optional for the mock provider but required for hosted models.
AI_AUTOCAD_API_KEY=
